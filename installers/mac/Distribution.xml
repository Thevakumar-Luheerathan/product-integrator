<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="1">
    <title>WSO2 Integrator</title>
    <organization>com.wso2.wso2integrator</organization>
    <domains enable_localSystem="true"/>
    <options customize="never" require-scripts="true" rootVolumeOnly="true" />
    <welcome file="welcome.html"/>
    <license file="license.txt"/>
    <conclusion file="conclusion.html"/>
    <choices-outline>
        <line choice="default">
            <line choice="com.wso2.wso2integrator"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="com.wso2.wso2integrator" visible="false">
        <pkg-ref id="com.wso2.wso2integrator"/>
    </choice>
    <pkg-ref id="com.wso2.wso2integrator" version="1.0.0" onConclusion="none">WSO2 Integrator.pkg</pkg-ref>
</installer-gui-script>
